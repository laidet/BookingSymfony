{% extends 'base.html.twig' %}

{% form_theme form 'ad/_collections.html.twig' %}

{% block body %}

    <div class="container">
        <h1>Création d'une nouvelle annonce</h1>

        {{ form_start(form) }}

            {{ form_widget(form) }}

            <button type="submit" class="btn btn-primary">Créer</button>

        {{ form_end(form) }}
    </div>
{% endblock %}
{% block form_row %}

    <div class="form-group">
        {{ form_label(form) }}
        {{ form_widget(form) }}
    </div>

{% endblock %}

